{
  "name": "workout-ai-trainer",
  "version": "1.0.0",
  "description": "POC to build an AI personal trainer to generate CrossFit-style workouts",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src -e ts,js --exec ts-node ./src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:unit": "vitest tests/unit --reporter=verbose",
    "test:integration": "vitest tests/integration --silent=false --reporter=verbose",
    "test:e2e": "vitest tests/e2e",
    "test:e2e:workout-flow": "vitest tests/e2e/workout-flow.test.ts",
    "test:e2e:performance": "vitest tests/e2e/performance.test.ts",
    "test:e2e:contract": "vitest tests/e2e/contract.test.ts",
    "test:auth": "ts-node scripts/test/auth-flow.ts",
    "test:auth-errors": "ts-node scripts/test/auth-errors.ts",
    "test:auth-middleware": "start-server-and-test dev http://localhost:3000/health 'npx ts-node -P scripts/tsconfig.json scripts/test/auth-middleware.ts'",
    "test:auth-all": "npm run test:auth && npm run test:auth-errors && npm run test:auth-middleware",
    "test:auth-with-server": "start-server-and-test dev http://localhost:3000 test:auth",
    "test:wod": "start-server-and-test dev http://localhost:3000/health 'npx ts-node -P scripts/tsconfig.json scripts/test/wod-endpoint.ts'",
    "test:wod-generation": "node --no-deprecation -r ts-node/register scripts/testWodGeneration.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:auth-all && npm run test:wod",
    "test:oauth": "ts-node -P scripts/tsconfig.json scripts/test/oauth-flow.ts",
    "test:oauth-with-server": "start-server-and-test dev http://localhost:3000/health test:oauth",
    "generate:wod": "ts-node scripts/generate_wod.ts",
    "generate:samples": "ts-node src/scripts/sample-wod-generation.ts",
    "generate-home-gym-wod": "ts-node src/scripts/generate-home-gym-wod.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanderzone/workout-ai-trainer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zanderzone/workout-ai-trainer/issues"
  },
  "homepage": "https://github.com/zanderzone/workout-ai-trainer#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/knex": "^0.15.2",
    "@types/mongodb": "4.0.7",
    "@types/node": "^22.13.1",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.3",
    "cross-env": "^7.0.3",
    "mongodb-memory-server": "^10.1.3",
    "mongoose": "7.6.0",
    "nodemon": "^3.1.9",
    "start-server-and-test": "^2.0.11",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0",
    "vitest": "^3.0.6",
    "zod-to-json-schema": "^3.24.1"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express-session": "^1.18.1",
    "@types/passport": "^1.0.17",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^5.0.0",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mongodb": "5.8.0",
    "openai": "^4.38.0",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.1",
    "zod": "^3.24.1"
  }
}